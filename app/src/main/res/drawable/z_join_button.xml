<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <padding android:bottom="4dp" android:left="8dp" android:right="8dp" android:top="4dp" />
            <solid android:color="#00E7AFC6" />
            <stroke android:color="#AAFFFFFF" android:width="1dp" />
            <corners android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp" android:topLeftRadius="6dp" android:topRightRadius="6dp" />
        </shape>
    </item>

</selector>